{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "dbauri",
    "version": "0.3.5"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false
      },
      "dialog": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "dbauri",
        "width": 1024,
        "height": 768,
        "resizable":true,
        "minimizable": true,
        "maximizable": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.dbauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
